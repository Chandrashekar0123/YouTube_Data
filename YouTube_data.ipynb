      }
    },
    {
      "cell_type": "code",
      "source": [
        "# Replace with your API Key\n",
        "API_KEY = 'AIzaSyBP-MmUNm-FgbMXWclmLSUdrTQHbpPI8Essdfg345hdfg'"
      ],
